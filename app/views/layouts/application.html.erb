      <div class="table">
        <tr>
          <td><%= link_to "food adventure"%></td>
          <td><%= link_to "outdoor fun"%></td>
          <td><%= link_to "local partner", partners_path%></td>
          <td><%= link_to "reservation"%></td>
          <td>
            <a href="https://www.facebook.com/chillchillkaohsiung"><i class="fa fa-facebook"></i></a>  
          </td>
          <td>
            <a href="http://instagram.com/chillchillkh"><i class="fa fa-instagram"></i></a>
          </td>
          <td>
            <a href="http://www.tripadvisor.com.tw/Attraction_Review-g297908-d6847667-Reviews-Chill_chill_Kaohsiung_project_Food_Tours_and_Taiwanese_Cooking_Class-Kaohsiung.html"><i class="fa fa-tripadvisor"></i></a>
